{
  "name": "queue-dispenser",
  "version": "3.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "dev": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.yourcompany.queuedispenser",
    "productName": "Queue Dispenser",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*"
    ],
    "extraResources": [
      {
        "from": "print.exe",
        "to": "print.exe"
      },
      {
        "from": "logs",
        "to": "logs"
      },
      {
        "from": "renderer",
        "to": "renderer"
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "favicon.png"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^39.2.6",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "ws": "^8.18.3"
  }
}
